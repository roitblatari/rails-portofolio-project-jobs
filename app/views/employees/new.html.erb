<h1>Please sign up</h1>
<%= form_for(@employee) do |f|%>
  <p> name: <%= f.text_field :name %></p>
  <p> email: <%= f.text_field :email %></p>
  <p> profession: <%= f.text_field :profession %></p>
  <p> password: <%= f.password_field :password  %></p>
  <%= f.submit %>
<% end %>
<h2><%= link_to 'Or Login Here', signin_employee_path %>  </h2>
